# Define the list
values = [3, 2, 1, 0]

# Use a for loop to print each value
for value in values:
    print(value)
# Assign values to guess_me and number
guess_me = 7
number = 1

# While loop for comparison
while True:
    if number < guess_me:
        print("too low")
    elif number == guess_me:
        print("found it!")
        break  # Exit the loop
    else:  # number > guess_me
        print("oops")
        break  # Exit the loop
    number += 1  # Increment number
# Assign value to guess_me
guess_me = 5

# For loop with range(10)
for number in range(10):
    if number < guess_me:
        print("too low")
    elif number == guess_me:
        print("found it!")
        break  # Exit the loop
    else:  # number > guess_me
        print("oops")
        break  # Exit the loop
